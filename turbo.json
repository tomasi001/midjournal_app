{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [".next/**", "dist/**", "build/**"],
      "dependsOn": ["^build"],
      "env": ["VAPID_PRIVATE_KEY"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "passThroughEnv": ["OLLAMA_HOST_OVERRIDE"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
